public class Solution {
    public int RemoveElement(int[] nums, int val) {
        List<int> list = new List<int>(nums);

        for(int i = 0; i < list.Count;i++){
            if(list[i] == val){
                list.RemoveAt(i);
                i--;
            }
        }

        for(int k = 0; k < list.Count; k++){
            nums[k] = list[k];
        }

        return list.Count;
    }
}
