public class Solution {
    public ListNode DeleteDuplicates(ListNode head) {
        ListNode tail = head;
     while(tail != null && tail.next != null){
           if(tail.val == tail.next.val){
           tail.next = tail.next.next;
        }
        else 
        tail = tail.next;
    }
     
        
    return head;
    }
        
}
