public class Solution {
    public void MoveZeroes(int[] nums) {
        int lastNonZero = 0; 

        
        for (int i = 0; i < nums.Length; i++) {
            if (nums[i] != 0) {
                
                int tmp = nums[lastNonZero];
                nums[lastNonZero] = nums[i];
                nums[i] = tmp;
                lastNonZero++;
            }
        }
    }
}
